{
    "name": "contentacms/contenta-jsonapi-project",
    "description": "Project template for ",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/liststyle",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/liststyle"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/liststyle/releases/liststyle_4.8.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "drupal/core": "8.8.5",
        "cweagans/composer-patches": "^1.6",
        "drush/drush": "~10.2.2",
        "contentacms/contenta_jsonapi": "v3.0.680",
        "drupal-composer/drupal-scaffold": "2.4.0",
        "drupal/fieldable_path": "^1.0@RC",
        "drupal/adminimal_theme": "^1.3",
        "drupal/migrate_plus": "~4.0",
        "drupal/migrate_tools": "~4.0",
        "drupal/restui": "^1.15",
        "drupal/pathauto": "1.6",
        "composer/installers": "^1.5",
        "ckeditor/liststyle": "4.8.0",
        "drupal/ckeditor_liststyle": "^1.3",
        "drupal/purge": "^3.0@beta",
        "drupal/varnish_purge": "^1.11",
        "drupal/tmgmt": "^1.7",
        "drupal/tmgmt_memsource": "^1.4",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/redirect": "^1.3",
        "drupal/field_group": "^3.0",
        "drupal/term_reference_tree": "1.x-dev",
        "drupal/paragraphs": "1.11",
        "drupal/pdf_api": "1.x-dev",
        "drupal/entity_browser": "2.5",
        "drupal/ief_table_view_mode": "^2.1",
        "drupal/paragraphs_browser": "1.0-beta1",
        "drupal/serial":"1.0-alpha1",
        "drupal/views_filters_populate":"1.1",
        "drupal/elasticsearch_connector": "^5.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.0",
        "drupal/drupal-extension": "^4.0",
        "smalot/pdfparser": "^0.15.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "dependency-cleanup": [
            "Drupal\\composer\\ScriptHandler::dependencyCleanup"
        ],
        "drupal-scaffold": "Drupal\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "Drupal\\composer\\ScriptHandler::buildScaffold",
            "Drupal\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "Drupal\\composer\\ScriptHandler::buildScaffold",
            "Drupal\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "patches":{
            "drupal/inline_entity_form": {
                "Issue #2653574: Unable to keep nested IEF data separate with multivalue fields" : "patches/contrib/inline_entity_form-2653574-141.patch"
            },
            "drupal/paragraphs_browser": {
                "Issue #3057621: Undefined index: paragraph_type_title" : "patches/contrib/undefined-index-paragraph_type_title-trait-3057621-7.patch"
            },
            "drupal/entity_browser": {
                "Issue #3117243: Undefined index: entity_browser_select" : "patches/contrib/warning_entity_browser_select-3117243.patch"
            },
            "drupal/views_filters_populate": {
                "Issue #3055963: Remove views_ui dependency" : "patches/contrib/remove-views-ui-dependency-3055963-v1.1.patch"
            }
        }
    },
    "config": {
        "preferred-install": {
            "asm89/stack-cors": "source",
            "*": "auto"
        },
        "bin-dir": "bin/"
    }
}
